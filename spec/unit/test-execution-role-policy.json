{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TestExtraExecutionRolePolicyStatement",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resources": ["*"]
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resources": ["arn:aws:logs:::*"]
    }
  ]
}
